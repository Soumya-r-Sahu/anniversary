{
  "name": "anniversary-website",
  "version": "3.0.0",
  "description": "A hyper-realistic anniversary website with React.js, Bootstrap, and advanced interactive features",
  "main": "src/main.tsx",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,html}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "analyze": "vite-bundle-analyzer",
    "optimize": "npm run lint:fix && npm run format && npm run type-check && npm run build",
    "deploy": "npm run build && npm run deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d dist"
  },
  "keywords": [
    "anniversary",
    "love",
    "website",
    "romantic",
    "modern",
    "performance",
    "react",
    "bootstrap",
    "threejs",
    "interactive",
    "hyper-realistic"
  ],
  "author": "Love Story Developers",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.158.3",
    "@types/howler": "^2.2.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-bundle-analyzer": "^0.7.0",
    "typescript": "^5.3.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "gh-pages": "^6.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "bootstrap": "^5.3.2",
    "react-bootstrap": "^2.9.2",
    "@react-three/fiber": "^8.15.12",
    "@react-three/drei": "^9.88.17",
    "three": "^0.158.0",
    "framer-motion": "^10.16.16",
    "gsap": "^3.12.2",
    "howler": "^2.2.4",
    "lottie-react": "^2.4.0",
    "canvas-confetti": "^1.9.2",
    "react-spring": "^9.7.3",
    "zustand": "^4.4.7",
    "react-hot-toast": "^2.4.1",
    "workbox-window": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}