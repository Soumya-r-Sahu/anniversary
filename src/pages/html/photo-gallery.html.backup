position: relative;
margin: 0 auto;
display: flex;
align-items: center;
justify-content: center;
}
.icon-beach::before {
content: '';
width: 20px;
height: 20px;
background: #fef3c7;
border-radius: 50%;
position: absolute;
top: 5px;
}

.icon-mountain {
width: 60px;
height: 60px;
position: relative;
margin: 0 auto;
display: flex;
align-items: flex-end;
justify-content: center;
}
.icon-mountain::before {
content: '';
width: 0;
height: 0;
border-left: 30px solid transparent;
border-right: 30px solid transparent;
border-bottom: 40px solid #6b7280;
position: absolute;
bottom: 0;
}
.icon-mountain::after {
content: '';
width: 0;
height: 0;
border-left: 15px solid transparent;
border-right: 15px solid transparent;
border-bottom: 20px solid #fff;
position: absolute;
bottom: 20px;
}

.icon-smile {
width: 60px;
height: 60px;
background: #fbbf24;
border-radius: 50%;
position: relative;
margin: 0 auto;
display: flex;
align-items: center;
justify-content: center;
}
.icon-smile::before {
content: 'â€¢ â€¢';
color: #000;
font-size: 20px;
position: absolute;
top: 15px;
}
.icon-smile::after {
content: '';
width: 30px;
height: 15px;
border: 3px solid #000;
border-top: none;
border-radius: 0 0 30px 30px;
position: absolute;
bottom: 15px;
}

.photo-gallery-hero {
background: linear-gradient(135deg, rgba(255, 107, 157, 0.1), rgba(255, 168, 204, 0.1));
backdrop-filter: blur(25px);
border-radius: 2rem;
padding: 4rem 2rem;
text-align: center;
border: 1px solid rgba(255, 107, 157, 0.2);
margin: 2rem 0;
position: relative;
overflow: hidden;
}

.photo-gallery-hero::before {
content: '';
position: absolute;
top: -50%;
left: -50%;
width: 200%;
height: 200%;
background: radial-gradient(circle, rgba(255, 107, 157, 0.1) 0%, transparent 70%);
animation: romantic-glow 4s ease-in-out infinite;
}

.page-icon-3d {
font-size: 5rem;
margin-bottom: 2rem;
position: relative;
z-index: 10;
animation: float-3d 4s ease-in-out infinite;
filter: drop-shadow(0 10px 20px rgba(255, 107, 157, 0.3));
}

.photo-grid-3d {
display: grid;
grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
gap: 2rem;
margin: 3rem 0;
perspective: 1000px;
}

.photo-card-3d {
background: rgba(255, 255, 255, 0.1);
backdrop-filter: blur(20px);
border-radius: 1.5rem;
padding: 2rem;
text-align: center;
border: 1px solid rgba(255, 255, 255, 0.2);
transition: all 0.6s cubic-bezier(0.175, 0.885, 0.32, 1.275);
transform-style: preserve-3d;
position: relative;
overflow: hidden;
}

.photo-card-3d:hover {
transform: rotateY(8deg) rotateX(4deg) translateY(-15px);
box-shadow:
0 25px 50px rgba(255, 107, 157, 0.3),
0 0 0 1px rgba(255, 255, 255, 0.3);
}

.photo-card-3d::before {
content: '';
position: absolute;
top: 0;
left: 0;
right: 0;
bottom: 0;
background: linear-gradient(135deg, rgba(255, 107, 157, 0.1), rgba(255, 168, 204, 0.1));
opacity: 0;
transition: opacity 0.3s ease;
}

.photo-card-3d:hover::before {
opacity: 1;
}

.coming-soon-3d {
background: linear-gradient(135deg, rgba(255, 255, 255, 0.15), rgba(255, 107, 157, 0.1));
backdrop-filter: blur(30px);
border-radius: 2rem;
padding: 4rem 3rem;
text-align: center;
border: 1px solid rgba(255, 107, 157, 0.3);
margin: 3rem 0;
position: relative;
transform-style: preserve-3d;
transition: all 0.5s ease;
}

.coming-soon-3d:hover {
transform: rotateY(5deg) rotateX(2deg) scale(1.02);
box-shadow: 0 30px 60px rgba(255, 107, 157, 0.2);
}

.construction-icon-3d {
font-size: 2.5rem;
margin: 0 0.5rem;
animation: pulse-3d 2s ease-in-out infinite;
filter: drop-shadow(0 5px 10px rgba(255, 107, 157, 0.3));
}

@keyframes float-3d {
0%, 100% {
transform: translateY(0px) rotateZ(0deg);
}
33% {
transform: translateY(-20px) rotateZ(2deg);
}
66% {
transform: translateY(-10px) rotateZ(-2deg);
}
}

@keyframes pulse-3d {
0%, 100% {
transform: scale(1) rotateZ(0deg);
opacity: 1;
}
50% {
transform: scale(1.1) rotateZ(5deg);
opacity: 0.8;
}
}

@keyframes romantic-glow {
0%, 100% {
opacity: 0.5;
transform: rotate(0deg) scale(1);
}
50% {
opacity: 0.8;
transform: rotate(180deg) scale(1.1);
}
}

.shimmer-overlay {
position: absolute;
top: -50%;
left: -50%;
width: 200%;
height: 200%;
background: linear-gradient(
45deg,
transparent 30%,
rgba(255, 255, 255, 0.2) 50%,
transparent 70%
);
transform: translateX(-100%);
transition: transform 0.6s ease;
}

.photo-card-3d:hover .shimmer-overlay {
transform: translateX(100%);
}

/* Photo Gallery Styles */
.gallery-hero {
text-align: center;
padding: 2rem 0;
margin-bottom: 3rem;
}

.photo-categories {
display: flex;
justify-content: center;
margin-bottom: 3rem;
}

.category-filters {
display: flex;
gap: 1rem;
flex-wrap: wrap;
justify-content: center;
}

.category-btn {
padding: 0.75rem 1.5rem;
background: rgba(255, 255, 255, 0.1);
backdrop-filter: blur(20px);
border: 1px solid rgba(255, 107, 157, 0.3);
border-radius: 2rem;
color: #ff6b9d;
font-weight: 600;
cursor: pointer;
transition: all 0.3s ease;
transform-style: preserve-3d;
display: flex;
align-items: center;
justify-content: center;
}

.category-btn i {
margin-right: 0.5rem;
}

.category-btn:hover, .category-btn.active {
background: rgba(255, 107, 157, 0.2);
transform: translateY(-2px) scale(1.05);
box-shadow: 0 10px 20px rgba(255, 107, 157, 0.3);
}

.photo-grid {
display: grid;
grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
gap: 2rem;
margin-bottom: 4rem;
}

.photo-item {
position: relative;
background: rgba(255, 255, 255, 0.1);
backdrop-filter: blur(25px);
border-radius: 1.5rem;
padding: 2rem;
border: 1px solid rgba(255, 107, 157, 0.2);
transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
transform-style: preserve-3d;
overflow: hidden;
}

.photo-item:hover {
transform: rotateY(5deg) rotateX(3deg) translateY(-10px) scale(1.02);
box-shadow: 0 20px 40px rgba(255, 107, 157, 0.3);
}

.photo-placeholder {
text-align: center;
position: relative;
z-index: 2;
}

.photo-icon {
font-size: 4rem;
margin-bottom: 1rem;
display: block;
filter: drop-shadow(0 5px 15px rgba(255, 107, 157, 0.3));
}

.photo-title {
font-size: 1.25rem;
font-weight: 600;
color: #ff6b9d;
margin-bottom: 0.5rem;
font-family: 'Dancing Script', cursive;
}

.photo-description {
color: #6b7280;
margin-bottom: 1rem;
line-height: 1.5;
}

.photo-date {
font-size: 0.875rem;
color: #9ca3af;
font-style: italic;
}

.photo-overlay {
position: absolute;
top: 0;
left: 0;
right: 0;
bottom: 0;
background: rgba(255, 107, 157, 0.9);
display: flex;
align-items: center;
justify-content: center;
opacity: 0;
transition: all 0.3s ease;
border-radius: 1.5rem;
}

.photo-item:hover .photo-overlay {
opacity: 1;
}

.view-photo-btn {
background: white;
color: #ff6b9d;
padding: 0.75rem 2rem;
border: none;
border-radius: 2rem;
font-weight: 600;
cursor: pointer;
transform: scale(0.8);
transition: all 0.3s ease;
}

.photo-item:hover .view-photo-btn {
transform: scale(1);
}

.view-photo-btn:hover {
transform: scale(1.1);
box-shadow: 0 5px 15px rgba(255, 255, 255, 0.3);
}

/* Photo Upload Section */
.photo-upload-section {
margin: 4rem 0;
text-align: center;
}

.upload-area {
border: 2px dashed rgba(255, 107, 157, 0.3);
border-radius: 1.5rem;
padding: 3rem;
background: rgba(255, 255, 255, 0.05);
backdrop-filter: blur(20px);
cursor: pointer;
transition: all 0.3s ease;
max-width: 500px;
margin: 0 auto;
}

.upload-area:hover {
border-color: rgba(255, 107, 157, 0.6);
background: rgba(255, 107, 157, 0.1);
transform: scale(1.02);
}

.upload-icon {
font-size: 3rem;
margin-bottom: 1rem;
opacity: 0.7;
}

.upload-title {
font-size: 1.25rem;
font-weight: 600;
color: #ff6b9d;
margin-bottom: 0.5rem;
}

.upload-subtitle {
color: #9ca3af;
font-size: 0.875rem;
}

/* Photo Statistics */
.photo-stats-section {
margin: 4rem 0;
}

.stats-grid {
display: grid;
grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
gap: 2rem;
max-width: 800px;
margin: 0 auto;
}

.stat-card {
background: rgba(255, 255, 255, 0.1);
backdrop-filter: blur(25px);
border-radius: 1.5rem;
padding: 2.5rem 2rem;
text-align: center;
border: 1px solid rgba(255, 107, 157, 0.2);
transition: all 0.5s ease;
transform-style: preserve-3d;
}

.stat-card:hover {
transform: rotateY(5deg) rotateX(3deg) translateY(-5px);
box-shadow: 0 15px 30px rgba(255, 107, 157, 0.2);
}

.stat-number {
font-size: 3rem;
font-weight: bold;
color: #ff6b9d;
margin-bottom: 0.5rem;
font-family: 'Dancing Script', cursive;
}

.stat-label {
font-size: 1.125rem;
font-weight: 600;
color: #4b5563;
margin-bottom: 0.5rem;
}

.stat-description {
font-size: 0.875rem;
color: #9ca3af;
line-height: 1.4;
}

/* Filter Animation */
.photo-item.filtered-out {
opacity: 0;
transform: scale(0.8);
pointer-events: none;
}

.photo-item.filtered-in {
opacity: 1;
transform: scale(1);
pointer-events: auto;
}
